<div class="m-t m-b">
Организаций в базе: <b>{{.DocumentsCount}}</b>
</div>
<ul>
{{range $i,$v := .orgs}}
<li><a href="/{{.OGRN}}">{{.FullName}}</a>
{{if eq $i 9}}
{{else}}<hr>{{end}}
{{end}}	
</ul>
<nav>
  <ul class="pagination">{{if .prevPage}}
    <li>
      <a href="/map" aria-label="Previous">
        <span aria-hidden="true">&laquo;</span>
        <span class="sr-only">Previous</span>
      </a>
    </li>{{end}}
    {{range .pagination}}
    <li {{if eq . $.currPage}}class="active"{{end}}><a href="/map?p={{.}}">{{.}}</a></li>
    {{end}}
    <li>
      <a href="/map?p={{.nextPage}}" aria-label="Next">
        <span aria-hidden="true">&raquo;</span>
        <span class="sr-only">Next</span>
      </a>
    </li>
    <li>
      <a href="/map?p={{.lastPage}}" aria-label="Next">
        <span aria-hidden="true">&raquo;&raquo;</span>
        <span class="sr-only">Next</span>
      </a>
    </li>
  </ul>
</nav>